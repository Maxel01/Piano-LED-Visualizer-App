    <menu text="Shutdown Options">
        <Shutdown_Options text="Shutdown">
            <Shutdown text="Confirm" action="send"></Shutdown>
            <Shutdown text="Cancel"></Shutdown>
        </Shutdown_Options>
        <Shutdown_Options text="Reboot">
            <Reboot text="Confirm" action="send"></Reboot>
            <Reboot text="Cancel"></Reboot>
        </Shutdown_Options>
    </menu>
