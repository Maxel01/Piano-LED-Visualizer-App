    <menu text="Other Settings">  <!-- TODO all -->
        <Other_Settings text="System Info" disabled="true"></Other_Settings>
        <Other_Settings text="Screensaver" disabled="true"> <!-- TODO Not yet available in App-->
            <Screensaver text="Content">
                <Content text="Time"></Content>
                <Content text="Date"></Content>
                <Content text="CPU Chart"></Content>
                <Content text="CPU"></Content>
                <Content text="RAM"></Content>
                <Content text="Temp"></Content>
                <Content text="Network Usage"></Content>
                <Content text="SD Card Space"></Content>
            </Screensaver>
            <Screensaver text="Start delay" disabled="true"> <!-- TODO Not yet available in App-->
                <Start_delay text="After minutes (0 - never)"></Start_delay>
            </Screensaver>
            <Screensaver text="Turn off screen delay" disabled="true"> <!-- TODO Not yet available in App-->
                <Turn_off_screen_delay text="After minutes (0 - never)"></Turn_off_screen_delay>
            </Screensaver>
            <Screensaver text="Led animation delay" disabled="true"> <!-- TODO Not yet available in App-->
                <Led_animation_delay text="After minutes (0 - never)"></Led_animation_delay>
            </Screensaver>
            <Screensaver text="Led animation">
                <Led_animation text="Theater Chase"></Led_animation>
                <Led_animation text="Breathing Slow"></Led_animation>
                <Led_animation text="Rainbow Slow"></Led_animation>
                <Led_animation text="Rainbow Cycle Slow"></Led_animation>
